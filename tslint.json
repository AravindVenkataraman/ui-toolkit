{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      "4"
    ],
    "ordered-imports": false,
    "object-literal-key-quotes": [
      false
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "prefer-const": false,
    "only-arrow-functions": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": false,
    "no-trailing-whitespace": false,
    "member-access": false,
    "no-console": false,
    "ban-types": false,
    "variable-name": [
      false
    ],
    "no-var-requires": false,
    "no-namespace": [false],
    "object-literal-shorthand": false,
    "no-shadowed-variable": false,
    "max-line-length": [
      1500
    ],
    "no-unused-expression": [
      false
    ],
    "forin": false,
    "space-before-function-paren": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "bin",
      "lib/*.js",
      "**/*.spec.ts",
      "dist"
    ]
  }
}
